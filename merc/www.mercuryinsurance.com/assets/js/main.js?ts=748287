".dropdown-content").each(function(){$(this).on("keydown",function(n){let i=n.currentTarget.querySelectorAll(".column"),f=n.target.parentElement,t=Array.prototype.indexOf.call(i,f),u=f.querySelectorAll("a"),r=Array.prototype.indexOf.call(u,n.target),e=$(".access-red")[0],o=$(".access-show")[0];if(n.keyCode===32&&(n.preventDefault(),n.target.click()),n.keyCode===27&&(closeSubMenu(e,o),n.currentTarget.previousElementSibling.focus()),n.keyCode===37){t===0&&(t=i.length);t-=1;let n=i[t].querySelectorAll("a");n.length-1<r&&(r=n.length-1);n[r].focus()}if(n.keyCode===38){n.preventDefault();let f;r!==0?u[r-1].focus():(t===0&&(t=i.length),f=i[t-1].querySelectorAll("a"),f[f.length-1].focus())}if(n.keyCode===39){t===i.length-1&&(t=-1);t+=1;let n=i[t].querySelectorAll("a");n.length-1<r&&(r=n.length-1);n[r].focus()}if(n.keyCode===40){n.preventDefault();let f;r!==u.length-1?u[r+1].focus():(t===i.length-1&&(t=-1),f=i[t+1].querySelectorAll("a"),f[0].focus())}if(n.keyCode===36&&(n.preventDefault(),i[0].querySelector("a").focus()),n.keyCode===35){n.preventDefault();let t=i[i.length-1].querySelectorAll("a");t[t.length-1].focus()}})});document.querySelectorAll(".row").forEach(n=>{let i=n.firstElementChild.querySelector("a"),t=i.innerText.toUpperCase();window.innerWidth>1047?(t==="INSURANCE OVERVIEW"||t==="RESOURCES")&&(i.style.display="none"):(t==="INSURANCE OVERVIEW"||t==="RESOURCES")&&(i.style.display="")});$(window).resize(function(){document.querySelectorAll(".row").forEach(n=>{let i=n.firstElementChild.querySelector("a"),t=i.innerText.toUpperCase();window.innerWidth>1047?(t==="INSURANCE OVERVIEW"||t==="RESOURCES")&&(i.style.display="none"):(t==="INSURANCE OVERVIEW"||t==="RESOURCES")&&(i.style.display="")})})
